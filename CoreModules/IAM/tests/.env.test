# Configurações de ambiente para testes do IAM
NODE_ENV=test
PORT=3001

# Configuração do banco de dados de teste
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=test
DB_PASSWORD=test
DB_DATABASE=iam_test
DB_SCHEMA=public
DB_SYNCHRONIZE=true
DB_LOGGING=false

# Configurações Redis para cache e sessões
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_PREFIX=iam_test

# Kafka para mensageria e integração
KAFKA_BROKERS=localhost:9092
KAFKA_CLIENT_ID=iam-test
KAFKA_GROUP_ID=iam-test-group

# Configuração JWT
JWT_SECRET=test-secret-key-for-development-only
JWT_EXPIRATION=3600
JWT_REFRESH_EXPIRATION=86400

# Configuração do servidor de teste
HOST=localhost
PORT=3001
API_PREFIX=/api
CORS_ORIGIN=*

# Integração com Risk Management
RISK_MANAGEMENT_API_URL=http://localhost:3002
RISK_MANAGEMENT_API_KEY=test-api-key

# Integração com Payment Gateway
PAYMENT_GATEWAY_API_URL=http://localhost:8080
PAYMENT_GATEWAY_API_KEY=test-payment-key

# Usuários de teste
DEFAULT_TENANT_ID=test-tenant
TEST_ADMIN_USERNAME=admin@test.com
TEST_ADMIN_PASSWORD=Admin123!
TEST_USER_USERNAME=user@test.com
TEST_USER_PASSWORD=User123!

# Configurações de log para testes
LOG_LEVEL=error
LOG_TO_FILE=false

# Configurações WebAuthn
WEBAUTHN_RP_ID=localhost
WEBAUTHN_RP_NAME=INNOVABIZ IAM Test
WEBAUTHN_RP_ORIGIN=http://localhost:3001

# TestContainers configuração
TESTCONTAINERS_RYUK_DISABLED=true