        "name": "PostgreSQL Exporter Errors",
        "step": "1m",
        "tagKeys": "tenant_id,region_id,environment,instance",
        "titleFormat": "PostgreSQL Exporter Error"
      },
      {
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "enable": true,
        "expr": "pg_replication_slot_active{tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", instance=~\"$instance\"} != 0",
        "iconColor": "rgba(255, 204, 0, 1)",
        "name": "Replication Slot Changes",
        "step": "1m",
        "tagKeys": "tenant_id,region_id,environment,instance",
        "titleFormat": "Replication Slot Change"
      }
    ]
  },
