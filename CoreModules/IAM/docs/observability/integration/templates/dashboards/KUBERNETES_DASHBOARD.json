{
  "dashboard": {
    "id": null,
    "title": "INNOVABIZ - Kubernetes Cluster Monitoring",
    "tags": ["kubernetes", "infrastructure", "innovabiz", "monitoring"],
    "style": "dark",
    "timezone": "UTC",
    "refresh": "30s",
    "schemaVersion": 39,
    "version": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "templating": {
      "list": [
        {
          "name": "tenant_id",
          "type": "query",
          "query": "label_values(kube_pod_info, tenant_id)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true,
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          }
        },
        {
          "name": "region_id",
          "type": "query",
          "query": "label_values(kube_pod_info{tenant_id=~\"$tenant_id\"}, region_id)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true,
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          }
        },
        {
          "name": "environment",
          "type": "query",
          "query": "label_values(kube_pod_info{tenant_id=~\"$tenant_id\", region_id=~\"$region_id\"}, environment)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true,
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          }
        },
        {
          "name": "cluster",
          "type": "query",
          "query": "label_values(kube_pod_info{tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\"}, cluster)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true,
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          }
        },
        {
          "name": "namespace",
          "type": "query",
          "query": "label_values(kube_pod_info{tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", cluster=~\"$cluster\"}, namespace)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true,
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          }
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Kubernetes Events",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "increase(kube_pod_container_status_restarts_total{tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", cluster=~\"$cluster\", namespace=~\"$namespace\"}[5m]) > 0",
          "iconColor": "red",
          "titleFormat": "Pod Restart",
          "textFormat": "{{namespace}}/{{pod}} restarted"
        },
        {
          "name": "Node Issues",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "kube_node_status_condition{condition=\"Ready\", status=\"false\", tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", cluster=~\"$cluster\"} == 1",
          "iconColor": "orange",
          "titleFormat": "Node Not Ready",
          "textFormat": "Node {{node}} is not ready"
        },
        {
          "name": "Resource Alerts",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "(kube_pod_container_resource_requests{resource=\"memory\", tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", cluster=~\"$cluster\", namespace=~\"$namespace\"} / kube_pod_container_resource_limits{resource=\"memory\"}) > 0.9",
          "iconColor": "yellow",
          "titleFormat": "High Memory Usage",
          "textFormat": "{{namespace}}/{{pod}} high memory usage"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Cluster Overview",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
        "collapsed": false
      },
      {
        "id": 2,
        "title": "Cluster Status",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 1},
        "targets": [
          {
            "expr": "up{job=\"kube-state-metrics\", tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", cluster=~\"$cluster\"}",
            "legendFormat": "Cluster Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            },
            "mappings": [
              {"options": {"0": {"text": "Down"}}, "type": "value"},
              {"options": {"1": {"text": "Up"}}, "type": "value"}
            ]
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "auto"
        }
      },
      {
        "id": 3,
        "title": "Total Nodes",
        "type": "stat",
        "gridPos": {"h": 4, "w": 3, "x": 6, "y": 1},
        "targets": [
          {
            "expr": "count(kube_node_info{tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", cluster=~\"$cluster\"})",
            "legendFormat": "Nodes"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 4,
        "title": "Ready Nodes",
        "type": "stat",
        "gridPos": {"h": 4, "w": 3, "x": 9, "y": 1},
        "targets": [
          {
            "expr": "sum(kube_node_status_condition{condition=\"Ready\", status=\"true\", tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", cluster=~\"$cluster\"})",
            "legendFormat": "Ready"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 5,
        "title": "Total Pods",
        "type": "stat",
        "gridPos": {"h": 4, "w": 3, "x": 12, "y": 1},
        "targets": [
          {
            "expr": "count(kube_pod_info{tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", cluster=~\"$cluster\", namespace=~\"$namespace\"})",
            "legendFormat": "Pods"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 6,
        "title": "Running Pods",
        "type": "stat",
        "gridPos": {"h": 4, "w": 3, "x": 15, "y": 1},
        "targets": [
          {
            "expr": "sum(kube_pod_status_phase{phase=\"Running\", tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", cluster=~\"$cluster\", namespace=~\"$namespace\"})",
            "legendFormat": "Running"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      }      },
      {
        "id": 7,
        "title": "CPU Usage by Node",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 1},
        "targets": [
          {
            "expr": "100 - (avg by (node) (irate(node_cpu_seconds_total{mode=\"idle\", tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", cluster=~\"$cluster\"}[5m])) * 100)",
            "legendFormat": "{{node}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 90}
              ]
            }
          }
        },
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"},
          "tooltip": {"mode": "multi", "sort": "none"}
        }
      },
      {
        "id": 8,
        "title": "Memory Usage by Node",
        "type": "timeseries",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 5},
        "targets": [
          {
            "expr": "(1 - (node_memory_MemAvailable_bytes{tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", cluster=~\"$cluster\"} / node_memory_MemTotal_bytes)) * 100",
            "legendFormat": "{{node}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 80},
                {"color": "red", "value": 95}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Disk Usage by Node",
        "type": "timeseries",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 5},
        "targets": [
          {
            "expr": "100 - ((node_filesystem_avail_bytes{mountpoint=\"/\", tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", cluster=~\"$cluster\"} * 100) / node_filesystem_size_bytes{mountpoint=\"/\"})",
            "legendFormat": "{{node}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 80},
                {"color": "red", "value": 95}
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "Pod Resources",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 9},
        "collapsed": false
      },
      {
        "id": 11,
        "title": "Pod CPU Usage",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 10},
        "targets": [
          {
            "expr": "sum(rate(container_cpu_usage_seconds_total{container!=\"POD\", container!=\"\", tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", cluster=~\"$cluster\", namespace=~\"$namespace\"}[5m])) by (namespace, pod)",
            "legendFormat": "{{namespace}}/{{pod}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 0.5},
                {"color": "red", "value": 1}
              ]
            }
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "right", "values": ["current", "max"]},
          "tooltip": {"mode": "multi", "sort": "desc"}
        }
      },
      {
        "id": 12,
        "title": "Pod Memory Usage",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 10},
        "targets": [
          {
            "expr": "sum(container_memory_working_set_bytes{container!=\"POD\", container!=\"\", tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", cluster=~\"$cluster\", namespace=~\"$namespace\"}) by (namespace, pod)",
            "legendFormat": "{{namespace}}/{{pod}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "bytes",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 536870912},
                {"color": "red", "value": 1073741824}
              ]
            }
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "right", "values": ["current", "max"]},
          "tooltip": {"mode": "multi", "sort": "desc"}
        }
      },
      {
        "id": 13,
        "title": "Network I/O",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 18},
        "targets": [
          {
            "expr": "sum(rate(container_network_receive_bytes_total{tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", cluster=~\"$cluster\", namespace=~\"$namespace\"}[5m])) by (namespace, pod)",
            "legendFormat": "{{namespace}}/{{pod}} - RX"
          },
          {
            "expr": "sum(rate(container_network_transmit_bytes_total{tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", cluster=~\"$cluster\", namespace=~\"$namespace\"}[5m])) by (namespace, pod)",
            "legendFormat": "{{namespace}}/{{pod}} - TX"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "Bps"
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "right", "values": ["current", "max"]},
          "tooltip": {"mode": "multi", "sort": "desc"}
        }
      },
      {
        "id": 14,
        "title": "Disk I/O",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 18},
        "targets": [
          {
            "expr": "sum(rate(container_fs_reads_bytes_total{tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", cluster=~\"$cluster\", namespace=~\"$namespace\"}[5m])) by (namespace, pod)",
            "legendFormat": "{{namespace}}/{{pod}} - Read"
          },
          {
            "expr": "sum(rate(container_fs_writes_bytes_total{tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", cluster=~\"$cluster\", namespace=~\"$namespace\"}[5m])) by (namespace, pod)",
            "legendFormat": "{{namespace}}/{{pod}} - Write"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "Bps"
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "right", "values": ["current", "max"]},
          "tooltip": {"mode": "multi", "sort": "desc"}
        }
      },
      {
        "id": 15,
        "title": "Workload Status",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 26},
        "collapsed": false
      },
      {
        "id": 16,
        "title": "Deployment Status",
        "type": "table",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 27},
        "targets": [
          {
            "expr": "kube_deployment_status_replicas{tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", cluster=~\"$cluster\", namespace=~\"$namespace\"}",
            "legendFormat": "{{namespace}}/{{deployment}} - Replicas",
            "format": "table"
          },
          {
            "expr": "kube_deployment_status_replicas_available{tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", cluster=~\"$cluster\", namespace=~\"$namespace\"}",
            "legendFormat": "{{namespace}}/{{deployment}} - Available",
            "format": "table"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "auto",
              "filterable": true
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Value #A"},
              "properties": [{"id": "displayName", "value": "Desired"}]
            },
            {
              "matcher": {"id": "byName", "options": "Value #B"},
              "properties": [{"id": "displayName", "value": "Available"}]
            }
          ]
        },
        "options": {
          "showHeader": true,
          "sortBy": [{"desc": false, "displayName": "namespace"}]
        }
      },
      {
        "id": 17,
        "title": "Pod Restarts",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 27},
        "targets": [
          {
            "expr": "increase(kube_pod_container_status_restarts_total{tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", cluster=~\"$cluster\", namespace=~\"$namespace\"}[1h])",
            "legendFormat": "{{namespace}}/{{pod}}/{{container}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "right", "values": ["current", "max"]},
          "tooltip": {"mode": "multi", "sort": "desc"}
        }
      },
      {
        "id": 18,
        "title": "Pod Status Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 27},
        "targets": [
          {
            "expr": "sum by (phase) (kube_pod_status_phase{tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", cluster=~\"$cluster\", namespace=~\"$namespace\"})",
            "legendFormat": "{{phase}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "pieType": "pie",
          "tooltip": {"mode": "single", "sort": "none"},
          "legend": {"displayMode": "visible", "placement": "right", "values": ["value", "percent"]},
          "displayLabels": ["name", "value"]
        }
      }
    ]
  }
}