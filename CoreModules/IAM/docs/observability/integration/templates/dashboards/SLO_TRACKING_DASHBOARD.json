        "name": "Error Budget Low",
        "titleFormat": "Orçamento de erro baixo para {{ $labels.service }}"
      },
      {
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "enable": true,
        "expr": "slo_availability{tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", service=~\"$service\"} < 0.999",
        "iconColor": "orange",
        "name": "SLO Violation",
        "titleFormat": "Violação de SLO para {{ $labels.service }}"
      },
      {
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "enable": true,
        "expr": "increase(slo_incidents_total{tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", service=~\"$service\"}[1h]) > 0",
        "iconColor": "yellow",
        "name": "New Incident",
        "titleFormat": "Novo incidente para {{ $labels.service }}"
      }
