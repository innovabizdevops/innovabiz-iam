        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      },
      {
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "enable": true,
        "expr": "timescaledb_up{tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", instance=~\"$instance\"} == 0",
        "iconColor": "red",
        "name": "TimescaleDB Down",
        "titleFormat": "TimescaleDB instância {{ $labels.instance }} offline"
      },
      {
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "enable": true,
        "expr": "timescaledb_compression_ratio{tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", instance=~\"$instance\"} < 0.3",
        "iconColor": "orange",
        "name": "Low Compression",
        "titleFormat": "Baixa taxa de compressão em {{ $labels.hypertable }}"
      },
      {
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "enable": true,
        "expr": "timescaledb_chunks_total{tenant_id=~\"$tenant_id\", region_id=~\"$region_id\", environment=~\"$environment\", instance=~\"$instance\"} > 10000",
        "iconColor": "yellow",
        "name": "High Chunk Count",
        "titleFormat": "Alto número de chunks em {{ $labels.hypertable }}"
      }
    ]
